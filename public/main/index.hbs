/* Add styles for the body when the cart is open */
/* Add styles for the body when the cart is open */
.cart-open {
    overflow: hidden;
}

.cart {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    display: none;
    justify-content: center;
    align-items: center;
    z-index: 9999;
}

.cart-container {
    background-color: white;
    padding: 20px;
    border-radius: 10px;
    max-width: 100%; /* Set maximum width */
    width: auto; /* Default width */
    height: 500px; /* Set height to auto for responsiveness */
    position: relative;
    overflow-y: auto; /* Enable vertical scrolling */
}

.close-cart {
    position: absolute;
    top: 10px;
    right: 10px;
    background-color: bisque;
    height: 40px;
    width: 60px;
    cursor: pointer;
}
.cart-item {
    display: flex;
    align-items: center; /* Align items vertically */
    margin-bottom: 10px; /* Adjust margin as needed */
}

.cart-item img {
    width: 100px; /* Set width of the image */
    height: auto; /* Maintain aspect ratio */
    margin-right: 20px; /* Adjust margin as needed */
}

.item-details {
    flex: 1; /* Allow the details to occupy remaining space */
}

.item-details h3 {
    margin-top: 0; /* Remove default margin */
    margin-bottom: 5px; /* Adjust margin as needed */
}

.item-details p {
    margin: 0; /* Remove default margin */
    font-size: 14px; /* Adjust font size as needed */
    line-height: 1.5; /* Adjust line height as needed */
}
/* Media query for smaller screens */
@media (max-width: 768px) {
    .cart {
        top: 50%; /* Adjust top position for smaller screens */
        left: 50%; /* Adjust left position for smaller screens */
        transform: translate(-50%, -50%);
    }

    .cart-container {
       
    max-width: max-content;
    padding: 10px;
    max-height: 80vh;
    overflow: auto; 
}


    .close-cart {
        bottom: 10px;
        right: 10px;
    }

    .cart-item img {
        max-width: 100%; /* Make the image responsive */
        height: auto;
    }

    .cart-item {
        display: flex;
        align-items: flex-end; /* Align elements data to the flex end */
        justify-content: space-between;
       
    }
}


