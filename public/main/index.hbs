<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
$(document).ready(function(){

     // Open cart when the shopping cart icon is clicked
    $(".fa-shopping-cart").click(function(){
        $("body").addClass("cart-open"); // Add class to body to fix background
        $(".cart").fadeIn();
    });

    // Close cart when the close button or background overlay is clicked
    $(".close-cart, .cart").click(function(event){
        if ($(event.target).hasClass('cart') || $(event.target).hasClass('close-cart')) {
            $("body").removeClass("cart-open"); // Remove class from body to allow scrolling
            $(".cart").fadeOut();
        }
    });

    // Add click event to order now button
    $(".button").click(function(){
        // Get the relevant information from the clicked div
        var imageSrc = $(this).closest('.gieYfx').find('.fyZwWD').attr('src');
        var itemName = $(this).closest('.gieYfx').find('.dFwWJC').text();
        var cuisineType = $(this).closest('.gieYfx').find('.fDjWNG').text();
        var priceText = $(this).closest('.gieYfx').find('.dHtbEm').text();
        var location = $(this).closest('.gieYfx').find('.cOAsXn').text();
        var openingHours = $(this).closest('.gieYfx').find('.iosCRN').text();

        // Create HTML for the item to be added to the cart
        var itemHTML = '<div class="cart-item" data-price="' + priceText + '">' +
                            '<img src="' + imageSrc + '" alt="Item Image">' +
                            '<div class="item-details">' + // Wrap the details in a div
                                '<h3>' + itemName + '</h3>' +
                                '<p>' + cuisineType + '</p>' +
                                '<p>' + priceText + '</p>' +
                                '<p>' + location + '</p>' +
                                '<p>' + openingHours + '</p>' +
                            '</div>' +
                            '<button class="remove-item">Remove</button>' + // Add remove button
                        '</div>';

        // Append the item to the cart
        $(".cart-items").append(itemHTML);
        $(".cart-items .cart-item:last-child").css("border-bottom", "1px solid #ccc");
        
        // Extract the price from the text and convert it to an integer
        var price = parseInt(priceText.match(/\d+/)[0]);

        // Update the total price
        var totalMoney = parseInt($(".total-price").text());
        totalMoney += price;
        $(".total-price").text(totalMoney);
    });

    // Add click event for remove item button
    $(document).on("click", ".remove-item", function() {
        // Get the price of the item being removed
        var priceText = $(this).closest('.cart-item').data('price');
        console.log("Price Text: " + priceText); // Debugging
        var price = parseInt(priceText.match(/\d+/)[0]);
        
        // Update the total price
        var totalMoney = parseInt($(".total-price").text());
        totalMoney -= price;
        $(".total-price").text(totalMoney);

        // Remove the item from the cart
        $(this).closest(".cart-item").remove();
    });
});

</script>
