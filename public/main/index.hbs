//array to save all the data to array to send it to the reciver website api   
$(".checkout-button").click(function(){
    // Collect all cart items data
    var cartItems = [];
    $(".cart-item").each(function(){
        var item = {
            imageSrc: $(this).find('img').attr('src'),
            itemName: $(this).find('h3').text(),
            cuisineType: $(this).find('p:eq(0)').text(),
            priceText: $(this).find('p:eq(1)').text(),
            location: $(this).find('p:eq(2)').text(),
            openingHours: $(this).find('p:eq(3)').text(),
        };
        cartItems.push(item);
    });

    // Collect selected date
    var selectedDate = $("#selected-date").text().replace("Selected Date: ", "");

    // Collect selected time
    var selectedTime = $("#selected-time").text().replace("Selected Time: ", "");

    // Collect packing/dining option
    var packingDiningOption = $("input[name='exampleRadios']:checked").val();

    // Collect total money
    var totalMoney = $(".total-price").text();

    // Send data to server using AJAX
    $.ajax({
        type: "POST",
        url: "/index",
        data: JSON.stringify({ 
            cartItems: cartItems,
            selectedDate: selectedDate,
            selectedTime: selectedTime,
            packingDiningOption: packingDiningOption,
            totalMoney: totalMoney
        }),
        contentType: "application/json",
        success: function(response) {
            console.log("Data sent to MongoDB successfully:", response);
            alert("Order saved Successfully");
         window.location.href = '/payment'; 
           
        },
        error: function(xhr, status, error) {
            console.error("Error sending data to MongoDB:", error);
            // Optionally, you can handle errors here
        }
    });
   


// Store data in sessionStorage 
    sessionStorage.setItem("cartItems", JSON.stringify(cartItems));
    sessionStorage.setItem("selectedDate", selectedDate);
    sessionStorage.setItem("selectedTime", selectedTime);
    sessionStorage.setItem("packingDiningOption", packingDiningOption);
    sessionStorage.setItem("totalMoney", totalMoney);

    
});



